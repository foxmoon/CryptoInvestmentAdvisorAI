#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/extdisk/project/web3/CryptoInvestmentAdvisorAI/node_modules/.pnpm/react-native@0.75.3_@babel+core@7.25.2_@babel+preset-env@7.25.4_@babel+core@7.25.2__bufferuti_znenvrj6ugu4qwvng43iy625by/node_modules/react-native/node_modules:/Volumes/extdisk/project/web3/CryptoInvestmentAdvisorAI/node_modules/.pnpm/react-native@0.75.3_@babel+core@7.25.2_@babel+preset-env@7.25.4_@babel+core@7.25.2__bufferuti_znenvrj6ugu4qwvng43iy625by/node_modules:/Volumes/extdisk/project/web3/CryptoInvestmentAdvisorAI/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Volumes/extdisk/project/web3/CryptoInvestmentAdvisorAI/node_modules/.pnpm/react-native@0.75.3_@babel+core@7.25.2_@babel+preset-env@7.25.4_@babel+core@7.25.2__bufferuti_znenvrj6ugu4qwvng43iy625by/node_modules/react-native/node_modules:/Volumes/extdisk/project/web3/CryptoInvestmentAdvisorAI/node_modules/.pnpm/react-native@0.75.3_@babel+core@7.25.2_@babel+preset-env@7.25.4_@babel+core@7.25.2__bufferuti_znenvrj6ugu4qwvng43iy625by/node_modules:/Volumes/extdisk/project/web3/CryptoInvestmentAdvisorAI/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../react-native@0.75.3_@babel+core@7.25.2_@babel+preset-env@7.25.4_@babel+core@7.25.2__bufferuti_znenvrj6ugu4qwvng43iy625by/node_modules/react-native/cli.js" "$@"
else
  exec node  "$basedir/../../../../../../react-native@0.75.3_@babel+core@7.25.2_@babel+preset-env@7.25.4_@babel+core@7.25.2__bufferuti_znenvrj6ugu4qwvng43iy625by/node_modules/react-native/cli.js" "$@"
fi
