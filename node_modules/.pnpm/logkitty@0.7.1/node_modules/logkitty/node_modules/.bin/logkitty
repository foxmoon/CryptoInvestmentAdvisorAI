#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/extdisk/project/web3/CryptoInvestmentAdvisorAI/node_modules/.pnpm/logkitty@0.7.1/node_modules/logkitty/bin/node_modules:/Volumes/extdisk/project/web3/CryptoInvestmentAdvisorAI/node_modules/.pnpm/logkitty@0.7.1/node_modules/logkitty/node_modules:/Volumes/extdisk/project/web3/CryptoInvestmentAdvisorAI/node_modules/.pnpm/logkitty@0.7.1/node_modules:/Volumes/extdisk/project/web3/CryptoInvestmentAdvisorAI/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Volumes/extdisk/project/web3/CryptoInvestmentAdvisorAI/node_modules/.pnpm/logkitty@0.7.1/node_modules/logkitty/bin/node_modules:/Volumes/extdisk/project/web3/CryptoInvestmentAdvisorAI/node_modules/.pnpm/logkitty@0.7.1/node_modules/logkitty/node_modules:/Volumes/extdisk/project/web3/CryptoInvestmentAdvisorAI/node_modules/.pnpm/logkitty@0.7.1/node_modules:/Volumes/extdisk/project/web3/CryptoInvestmentAdvisorAI/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/logkitty.js" "$@"
else
  exec node  "$basedir/../../bin/logkitty.js" "$@"
fi
