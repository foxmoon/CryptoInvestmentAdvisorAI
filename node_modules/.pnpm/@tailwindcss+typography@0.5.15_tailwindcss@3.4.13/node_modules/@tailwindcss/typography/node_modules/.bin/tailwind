#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/extdisk/project/web3/CryptoInvestmentAdvisorAI/node_modules/.pnpm/tailwindcss@3.4.13/node_modules/tailwindcss/lib/node_modules:/Volumes/extdisk/project/web3/CryptoInvestmentAdvisorAI/node_modules/.pnpm/tailwindcss@3.4.13/node_modules/tailwindcss/node_modules:/Volumes/extdisk/project/web3/CryptoInvestmentAdvisorAI/node_modules/.pnpm/tailwindcss@3.4.13/node_modules:/Volumes/extdisk/project/web3/CryptoInvestmentAdvisorAI/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Volumes/extdisk/project/web3/CryptoInvestmentAdvisorAI/node_modules/.pnpm/tailwindcss@3.4.13/node_modules/tailwindcss/lib/node_modules:/Volumes/extdisk/project/web3/CryptoInvestmentAdvisorAI/node_modules/.pnpm/tailwindcss@3.4.13/node_modules/tailwindcss/node_modules:/Volumes/extdisk/project/web3/CryptoInvestmentAdvisorAI/node_modules/.pnpm/tailwindcss@3.4.13/node_modules:/Volumes/extdisk/project/web3/CryptoInvestmentAdvisorAI/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../tailwindcss@3.4.13/node_modules/tailwindcss/lib/cli.js" "$@"
else
  exec node  "$basedir/../../../../../../tailwindcss@3.4.13/node_modules/tailwindcss/lib/cli.js" "$@"
fi
