else
  export NODE_PATH="/Volumes/extdisk/project/web3/CryptoInvestmentAdvisorAI/node_modules/.pnpm/next@14.2.13_@babel+core@7.25.2_react-dom@18.3.1_react@18.3.1__react@18.3.1/node_modules/next/dist/bin/node_modules:/Volumes/extdisk/project/web3/CryptoInvestmentAdvisorAI/node_modules/.pnpm/next@14.2.13_@babel+core@7.25.2_react-dom@18.3.1_react@18.3.1__react@18.3.1/node_modules/next/dist/node_modules:/Volumes/extdisk/project/web3/CryptoInvestmentAdvisorAI/node_modules/.pnpm/next@14.2.13_@babel+core@7.25.2_react-dom@18.3.1_react@18.3.1__react@18.3.1/node_modules/next/node_modules:/Volumes/extdisk/project/web3/CryptoInvestmentAdvisorAI/node_modules/.pnpm/next@14.2.13_@babel+core@7.25.2_react-dom@18.3.1_react@18.3.1__react@18.3.1/node_modules:/Volumes/extdisk/project/web3/CryptoInvestmentAdvisorAI/node_modules/.pnpm/node_modules:$NODE_PATH"
